	protected void flush() throws IOException {
		PrintWriter pw = null;
		try {
			pw = new PrintWriter(histFile);
			for (int i = 0; i < entries.size(); i++) {
				HistoryEntry entry = entries.get(i);
				pw.println(entry.toString());
			}
		} finally {
			if (pw != null) {
				pw.close();
			}
		}
	}

