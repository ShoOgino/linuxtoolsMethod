		@Override
		public int compare(IMarker marker1, IMarker marker2) {
			int line1 = marker1.getAttribute(IMarker.LINE_NUMBER, -1);
			int line2 = marker2.getAttribute(IMarker.LINE_NUMBER, -1);

			if (line1 > line2) {
				return 1;
			} else if (line2 < line1) {
				return -1;
			}
			return 0;
		}

