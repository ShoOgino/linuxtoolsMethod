		public String getColumnText(Object element, int columnIndex) {
			String text = null;
			IPeer peer = (IPeer) element;
			switch (columnIndex) {
			case 0:
				text = peer.getID();
				break;
			case 1:
				text = peer.getName();
				break;
			case 2:
				text = peer.getOSName();
				break;
			case 3:
				text = peer.getTransportName();
				break;
			}
			return text;
		}

