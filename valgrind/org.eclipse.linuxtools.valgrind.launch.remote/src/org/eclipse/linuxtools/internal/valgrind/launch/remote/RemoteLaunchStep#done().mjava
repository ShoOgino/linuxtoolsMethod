//		System.out.println("End: " + name);
		
		if (channel.getState() != IChannel.STATE_OPEN) {
			return;
		}
        try {
        	if (!steps.isEmpty()) {
        		RemoteLaunchStep step = steps.remove();
        		
//        		printSpaces();
//				indent++;
//        		System.out.println("Begin: " + step.name);
        		
				step.start();
        	}
        }
        catch (Throwable x) {
            channel.terminate(x);
        }
	}

