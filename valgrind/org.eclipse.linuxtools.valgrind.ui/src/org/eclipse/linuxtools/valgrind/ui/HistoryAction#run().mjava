	@Override
	public void run() {
		if (dir.exists()) {
			try {
				IValgrindLaunchDelegate delegate = ValgrindLaunchPlugin.getDefault().getToolDelegate(entry.getTool());
				delegate.reparseOutput(dir);
				HistoryFile.getInstance().moveToEnd(entry);
			} catch (CoreException e) {
				e.printStackTrace();
			} catch (IOException e) {
				e.printStackTrace();
			}			
		}
	}

