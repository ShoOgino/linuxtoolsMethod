	protected Long parseLongValue(String line, String delim)
			throws IOException {
				Long result = null;
				String[] parts = line.split(delim);
				if (parts.length > 1 && isNumber(parts[1])) {
					result = Long.parseLong(parts[1]);
				}
				else {
					fail(line);
				}
				return result;
			}

