	protected void createDataDir() throws IOException {
		if (dataDir.exists()) {
			// delete any preexisting files
			deleteFiles();
		}
		else if (!dataDir.mkdir()) {
			dataDir = null;
			throw new IOException(NLS.bind(Messages.getString("ValgrindCommand.Couldnt_create"), DATA_PATH)); //$NON-NLS-1$
		}
	}

