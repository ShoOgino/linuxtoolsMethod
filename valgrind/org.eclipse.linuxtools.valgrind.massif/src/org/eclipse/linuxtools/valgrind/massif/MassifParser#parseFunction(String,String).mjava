	private String parseFunction(String start, String line) throws IOException {
		String function = null;
		int ix = line.lastIndexOf("("); //$NON-NLS-1$
		if (ix >= 0) {
			function = line.substring(line.indexOf(start), ix);
			if (function != null) {
				function = function.trim();
			}
			else {
				fail(line);
			}
		}
		else {
			fail(line);
		}
		
		return function;
	}

