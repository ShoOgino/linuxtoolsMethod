		configuration.setAttribute(ATTR_MASSIF_DETAILEDFREQ, 10);
		configuration.setAttribute(ATTR_MASSIF_MAXSNAPSHOTS, 100);
		configuration.setAttribute(ATTR_MASSIF_ALIGNMENT, 8);
	}

