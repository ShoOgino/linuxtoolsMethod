	/**
	 * @see org.eclipse.cdt.oprofile.core.ISampleContainer#getSampleCount()
	 */
	public int getSampleCount() {
		if (_count < 0) {
			_count = 0;
			for (Iterator i = _samples.iterator(); i.hasNext(); ) {
				Sample s = (Sample) i.next();
				_count += s.getSampleCount();
			}
		}
		
		return _count;
	}

