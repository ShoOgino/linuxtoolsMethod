    private String getLibraryName(String lib) {
        // /lib64/libc-2.12.so - libc-2.12.s0
        String libName = ""; //$NON-NLS-1$
        int index = 0;
        if (null != lib && lib.trim().length() != 0) {
            index = lib.lastIndexOf('/');
            if (index != -1) {
                libName = lib.substring(index + 1, lib.length());
            }

        }
        return libName;
    }

