	public void refreshModel() {
		//TODO-performance/interactivity: some persistence for events/sessions
		// that dont change from run to run (non default sessions) 
		
		events = getNewEvents();
		if (events != null) {
			for (int i = 0; i < events.length; i++) {
				if (events[i] != null)
					events[i].refreshModel();
			}
		}
	}

