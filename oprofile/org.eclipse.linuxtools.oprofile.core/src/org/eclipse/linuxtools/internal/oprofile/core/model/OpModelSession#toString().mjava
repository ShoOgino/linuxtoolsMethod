    @Override
    public String toString() {
        String s = name + "\n"; //$NON-NLS-1$
        if (events != null) {
            for (int i = 0; i < events.length; i++) {
                s += printTabs + "Event: "; //$NON-NLS-1$
                s += events[i].toString(printTabs + "\t"); //$NON-NLS-1$
            }
        }
        return s;

    }

