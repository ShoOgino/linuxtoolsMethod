    private void setReturnCode() {
        if (! isValidCounter()){
            returnCode = "invalid-counter"; //$NON-NLS-1$
            return;
        }

        if (! isValidUnitMask()){
            returnCode = "invalid-um"; //$NON-NLS-1$
            return;
        }

        returnCode = "ok"; //$NON-NLS-1
    }

