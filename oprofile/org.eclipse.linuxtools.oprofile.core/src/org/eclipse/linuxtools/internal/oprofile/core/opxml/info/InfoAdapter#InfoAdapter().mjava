	public InfoAdapter (){
		try {
			if (hasTimerSupport()){
				// In timer mode, we have no relevant XML generated by ophelp
				createDOM(null);
			}else{
				Process p = RuntimeProcessFactory.getFactory().exec("ophelp -X", Oprofile.OprofileProject.getProject());
				InputStream is = p.getInputStream();
				createDOM(is);
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

