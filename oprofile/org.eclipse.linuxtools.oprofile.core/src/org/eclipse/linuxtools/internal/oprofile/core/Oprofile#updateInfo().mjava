	/**
	 * @since 1.1
	 */
	public static void updateInfo(){
		try {
			if (!OprofileCorePlugin.getDefault().getOpcontrolProvider().status()){
				OprofileCorePlugin.getDefault().getOpcontrolProvider().initModule();
			}
			info = OpInfo.getInfo();
		} catch (OpcontrolException e) {
			e.printStackTrace();
		}

	}

