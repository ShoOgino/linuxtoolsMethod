    private static FunctionDef FindFunctionDef(String name, List<FunctionDef> FDefs) {
        for (Iterator<FunctionDef> iterator = FDefs.iterator(); iterator.hasNext();) {
            FunctionDef k = iterator.next();
            if (k.FunctionName.equals(name)) {
                return k;
            }
        }
        return null;
    }

