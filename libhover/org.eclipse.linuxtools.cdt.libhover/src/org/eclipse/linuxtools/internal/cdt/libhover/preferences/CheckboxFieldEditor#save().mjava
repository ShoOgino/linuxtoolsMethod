	private void save() {
		boolean inRange = true;
		int i = 0;
		String outString = "";
		String del = "";
		while (inRange) {
			TableData t;
			Object obj = tv.getElementAt(i);
			if (obj == null)
				inRange = false;
			else {
				++i;
				t = (TableData)obj;
				outString += del + t.key + ":" + tv.getChecked(obj);
				del = ":";
			}
		}
		if (outString.length() > 1)
			getPreferenceStore().setValue(preferenceName, outString);
	}

