	/**
	 * Return the require paths
	 *
	 * @return The require paths
	 */
	public List<String> getRequirePaths() {
		List<String> rc = new ArrayList<String>();
		List<String> temp = rubyGemParser.getValueList(REQUIRE_PATHS);

		if (!temp.isEmpty()) {
			for (String tmp : temp) {
				rc.add(tmp
						.replaceFirst("(?:%q|%Q)(?:([\\W])([^\\W]+)[\\W])",
								"$2").replaceAll("(\"|'|\\[|\\])", "")
						.replaceAll(",", ""));
			}
		}

		return rc;
	}

