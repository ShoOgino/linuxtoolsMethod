    /*
     * (non-Javadoc)
     * @see org.eclipse.jface.preference.PathEditor#parseString(java.lang.String)
     */
    @Override
    protected String[] parseString(String stringList) {
        if (!stringList.isEmpty()) {
            return stringList.split(ICreaterepoConstants.DELIMITER);
        }
        return new String[]{};
    }

