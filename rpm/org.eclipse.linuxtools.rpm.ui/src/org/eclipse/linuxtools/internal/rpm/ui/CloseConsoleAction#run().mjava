    /**
     * Closes the console.
     */
    @Override
    public void run() {
        PlatformUI.getWorkbench().getDisplay().syncExec(new Runnable() {
            @Override
            public void run() {
                if(fConsole != null){
                    ConsolePlugin.getDefault().getConsoleManager().removeConsoles(new IConsole[]{fConsole});
                }
            }
        });
    }

