    @Override
    public Object execute(ExecutionEvent event) {
        final IResource resource = getResource(event);
        rpj = getRPMProject(resource);
        if (rpj != null) {
            Job job = new RPMExportOperation(rpj, event.getParameter(BUILD_TYPE));
            job.setUser(true);
            job.schedule();
        }
        return null;
    }

