	public void matchFound(PatternMatchEvent event) {
		String line = null;
		try {
			line = console.getDocument().get(event.getOffset(),
					event.getLength());
			console.addMissingRpm(line.substring(0, line.indexOf("is needed by")).trim()); //$NON-NLS-1$
		} catch (BadLocationException e1) {
			return;
		}

	}

