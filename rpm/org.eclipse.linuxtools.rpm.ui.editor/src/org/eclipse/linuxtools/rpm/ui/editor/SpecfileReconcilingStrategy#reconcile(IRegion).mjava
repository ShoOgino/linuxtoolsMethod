	public void reconcile(IRegion partition) {
		try {
			Specfile specfile = editor.getSpecfile();
			SpecfileParser parser = editor.getParser();
			if (specfile != null) {
				editor.setSpecfile(parser.parse(documentProvider
						.getDocument(editor.getEditorInput())));
				outline.update();
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

