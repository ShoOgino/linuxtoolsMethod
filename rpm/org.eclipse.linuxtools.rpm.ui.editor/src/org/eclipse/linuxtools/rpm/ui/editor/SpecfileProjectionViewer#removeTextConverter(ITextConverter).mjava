	/**
	 * Remove the given text converter
	 * 
	 * @param textConverter
	 *            to remove
	 */
	public void removeTextConverter(ITextConverter textConverter) {
		if (textConverters != null) {
			textConverters.remove(textConverter);
			if (textConverters.size() == 0)
				textConverters = null;
		}
	}

