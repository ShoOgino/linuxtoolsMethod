    /**
     * Adding the page to the wizard.
     */
    @Override
    public void addPages() {
        if (!Utils.fileExist("/usr/bin/rpmdev-newspec")) { //$NON-NLS-1$
            addPage(new NoExecutableWizardPage());
        } else {
            page = new SpecfileNewWizardPage(selection);
            addPage(page);
        }
    }

