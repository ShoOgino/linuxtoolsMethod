	/**
	 * @param input
	 * @return
	 */
	private String[][] stripFirstColumn (String[][] input) {
		String output[][] = null;
		if (null != input) {
			output = new String[input.length][input[0].length-1];
			for (int j = 0; j < output.length; j++) {
				for (int i = 0; i < output[0].length; i++) {
					output[j][i] = input[j][i+1];
				}
			}
		}
		return output;
	}

