	/**
	 * 
	 */
	public void start() {
		try {
			if (null != processor) {
				if (ToolStateEnum.STOPPED == toolState) {
					toolState = ToolStateEnum.RUNNING;
					processor.gatherData();
				} else if (ToolStateEnum.STOP_SCHEDULED == toolState) {
					toolState = ToolStateEnum.RUNNING;
				}
			}
		} catch (IOException ie) {
			//TODO: Nothing ?
		}
	}

