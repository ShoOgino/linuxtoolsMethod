	/**
	 * * Removes docker image.
	 */
	public void remove() {
		select();
		new ContextMenu("Remove").select();

		Shell confirm = new DefaultShell("Confirm Remove Image");
		new OkButton().click();

		new WaitWhile(new ShellIsAvailable(confirm));
		new WaitWhile(new JobIsRunning(), TimePeriod.LONG);
	}

