	/* (non-Javadoc)
	 * @see org.jboss.reddeer.workbench.api.View#open()
	 */
	@Override
	public void open() {
		log.info("Open view " + getTitle());
		// view is not opened, it has to be opened via menu
		if (getViewCTabItem() == null){
			log.info("Open " + getTitle() + " view via menu.");
			openViaMenu();
		}
		activate();
	}

