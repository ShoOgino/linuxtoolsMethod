	/*
	 * Access arbitrarily nested internal fields.
	 */
	private static Object getInternalField (Object input, List<String []> set) {
		Object curr = input;
		try {
			for (String [] e : set) {
				Field f = loadClass(e[0]).getDeclaredField(e[1]);
				f.setAccessible(true);
				curr = f.get(curr);
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		return curr;
	}

