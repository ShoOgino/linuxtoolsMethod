	@Override
	public void close() {
		// stop and remove all logging threads
		for (String key : loggingThreads.keySet()) {
			LogThread t = loggingThreads.get(key);
			if (t != null) {
				t.kill();
			}
			loggingThreads.remove(key);
		}
		synchronized (clientLock) {
			if (this.client != null) {
				this.client.close();
				this.client = null;
			}
			setState(EnumDockerConnectionState.CLOSED);
		}
	}

