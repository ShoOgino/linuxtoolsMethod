	/*
	 * Shift contents of buff[idex] .. buff[buff.length-1] to buff[0] ..
	 * buff[(buff.length-1) - idex]
	 */
	private static void shiftLeft(byte[] buff, int idex) {
		for (int i = 0; i < buff.length; i++) {
			if (idex + i < buff.length) {
				buff[i] = buff[idex + i];
			} else {
				buff[i] = 0;
			}
		}
	}

