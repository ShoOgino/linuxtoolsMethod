	@Override
	public List<IDockerContainer> getContainers(final boolean force) {
		if (!isActive()) {
			return Collections.emptyList();
		}
		if (!isContainersLoaded() || force) {
			try {
				return listContainers();
			} catch (DockerException e) {
				Activator.log(e);
			}
		}
		return this.containers;
	}

