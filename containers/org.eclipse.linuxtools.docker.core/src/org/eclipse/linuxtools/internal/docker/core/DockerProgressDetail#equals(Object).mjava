	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		DockerProgressDetail other = (DockerProgressDetail) obj;
		return this.current == other.current && this.start == other.start
				&& this.total == other.total;
	}

