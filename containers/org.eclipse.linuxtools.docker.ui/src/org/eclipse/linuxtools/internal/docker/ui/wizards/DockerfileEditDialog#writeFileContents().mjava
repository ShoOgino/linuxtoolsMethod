	private void writeFileContents() throws IOException {
		try {
			BufferedOutputStream output = new BufferedOutputStream(
					dockerFile.openOutputStream(EFS.NONE, null));
			byte[] bytes = textArea.getText().getBytes();
			if (bytes.length > 0)
				output.write(bytes);
			output.close();
		} catch (CoreException e) {
			e.printStackTrace();
		}

	}

