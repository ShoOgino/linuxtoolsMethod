	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result
				+ ((containerPath == null) ? 0 : containerPath.hashCode());
		result = prime * result + ((mount == null) ? 0 : mount.hashCode());
		result = prime * result
				+ ((mountType == null) ? 0 : mountType.hashCode());
		result = prime * result + (readOnly ? 1231 : 1237);
		return result;
	}

