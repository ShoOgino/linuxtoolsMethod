	@Override
	public void dispose() {
		for (Job job : openRetryJobs.values()) {
			LoadingJob loadingJob = (LoadingJob) job;
			IProgressMonitor monitor = loadingJob.getMonitor();
			monitor.setCanceled(true);
			job.cancel();
			try {
				job.join();
			} catch (InterruptedException e) {
				// ignore
			}
		}
	}

