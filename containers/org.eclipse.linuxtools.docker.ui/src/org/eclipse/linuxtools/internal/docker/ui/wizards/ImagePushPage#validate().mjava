	private void validate() {
		boolean complete = true;
		boolean error = false;
		String name = null;
		if (nameText != null) {
			name = nameText.getText();
		} else {
			name = nameCombo.getText();
		}
		if (name.length() == 0) {
			complete = false;
		}
		if (!error) {
			setErrorMessage(null);
			tag = name;
		}
		setPageComplete(complete && !error);
	}

