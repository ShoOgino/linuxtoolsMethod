		@Override
		public void keyReleased(KeyEvent e) {
			if (ptyOutRef[0] != null && ptyOutRef[0].isOpen()) {
				WritableByteChannel pty_out = ptyOutRef[0];
				try {
					switch (e.keyCode) {
					case CTRL_CODE:
						isCtrlOn = false;
						break;
					case TAB_CODE:
						pty_out.write(ByteBuffer
								.wrap(new byte[] { 9, 9 }, 0, 2));
						break;
					}
				} catch (IOException e1) {
				}
			}
		}

