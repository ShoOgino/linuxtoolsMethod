	private Long getDigitalValue(String s) {
		s = s.trim();
		int len = s.length();
		long factor = 1;
		if (len > 0) {
			char c = s.charAt(len - 1);
			boolean foundSpecifier = false;
			switch (c) {
			case 'k':
				factor = 1000;
				foundSpecifier = true;
				break;
			case 'K':
				factor = 1024;
				foundSpecifier = true;
				break;
			case 'm':
				factor = 1000000;
				foundSpecifier = true;
				break;
			case 'M':
				factor = 1024 * 1024;
				foundSpecifier = true;
				break;
			case 'g':
				factor = 1000000000;
				foundSpecifier = true;
				break;
			case 'G':
				factor = 1024 * 1024 * 1024;
				foundSpecifier = true;
				break;
			case 't':
				factor = 1000000000000L;
				foundSpecifier = true;
				break;
			case 'T':
				factor = 1024 * 1024 * 1024 * 1024;
				foundSpecifier = true;
				break;
			}
			if (foundSpecifier) {
				s = s.substring(len - 1);
			}
		}
		return Long.valueOf(Long.parseLong(s) * factor);
	}

