		@Override
		public int hashCode() {
			final int prime = 31;
			int result = 1;
			result = prime * result
					+ ((containerPath == null) ? 0 : containerPath.hashCode());
			result = prime * result + ((flags == null) ? 0 : flags.hashCode());
			result = prime * result
					+ ((hostPath == null) ? 0 : hostPath.hashCode());
			return result;
		}

