	protected Double getDoubleValue(Object o) {
		if (o == null) {
			return null;
		}
		if (o instanceof Integer) {
			return ((Integer)o).doubleValue();
		}
		if (o instanceof Double) {
			return (Double) o;
		}
		try {
			return new Double(o.toString());
		} catch (NumberFormatException e) {
			return null;
		}
	}

