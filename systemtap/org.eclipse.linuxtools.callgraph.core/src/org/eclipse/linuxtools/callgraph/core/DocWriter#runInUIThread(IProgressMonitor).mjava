	@Override
	public IStatus runInUIThread(IProgressMonitor monitor) {
		
		if (console == null)
			return Status.CANCEL_STATUS;
		
		IDocument doc = console.getDocument();
		
		try {
			doc.replace(doc.getLength(), 0, message);
		} catch (BadLocationException e) {
			e.printStackTrace();
		}
		
		return Status.OK_STATUS;
	}

