		@Override
		public IStatus run(IProgressMonitor monitor) {
			IStatus returnStatus = Status.CANCEL_STATUS;       
	        try {
	        	setInternalData();
	            while (!isDone){
	            	returnStatus = realTimeParsing();
	            	
	            	
	            	if (monitor.isCanceled() || returnStatus == Status.CANCEL_STATUS) {
	            		return Status.CANCEL_STATUS;
	            	}
	            }
		    } catch (Exception e) {
		       	e.printStackTrace();
	        }
			return returnStatus;
		}

