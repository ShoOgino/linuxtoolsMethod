	@Test
	public void testCommand() {
		assertNotNull("Command not null", tc);

		tc = new Command(null, null);
		assertNotNull("Command not null", tc);

		tc = new Command(new String[] {}, null);
		assertNotNull("Command not null", tc);

		tc = new Command(new String[] {""}, null);
		assertNotNull("Command not null", tc);

		tc = new Command(new String[] {"a"}, null);
		assertNotNull("Command not null", tc);

		tc = new Command(new String[] {"ls", "/"}, null);
		assertNotNull("Command not null", tc);
	}

