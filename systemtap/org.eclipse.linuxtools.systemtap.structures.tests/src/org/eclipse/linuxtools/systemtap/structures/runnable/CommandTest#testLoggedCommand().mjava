	@Test
	public void testLoggedCommand() {
		assumeTrue(stapInstalled);
		tc.dispose();

		tc = new Command(new String[] {"stap", "-v", "-p1", "-e", "probe nosuchfunc{}"}, null);
		tc.start();
		assertTrue(tc.isRunning());
		assertFalse(tc.isDisposed());
		tc.stop();
		assertFalse(tc.isRunning());
		assertFalse(tc.isDisposed());
		tc.dispose();

		tc = new Command(new String[] {"stap", "-v", "-p1", "-e", "probe nosuchfunc{}"}, null);
		tc.start();
		assertTrue(tc.isRunning());
		assertFalse(tc.isDisposed());
		tc.stop();
		assertFalse(tc.isRunning());
		assertFalse(tc.isDisposed());
		tc.dispose();
	}

