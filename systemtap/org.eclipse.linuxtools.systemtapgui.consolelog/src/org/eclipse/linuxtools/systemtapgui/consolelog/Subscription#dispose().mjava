	public void dispose() {
		if(!disposed) {
			disposed = true;
			inputListeners = null;
			errorListeners = null;

			if(null != inputGobbler){
				inputGobbler.dispose();
				inputGobbler.stop();
			}
			inputGobbler = null;
			
			if(null != errorGobbler){
				errorGobbler.dispose();
				errorGobbler.stop();
			}
			errorGobbler = null;
		}
	}

