	@Override
	protected void okPressed() {
		DashboardPlugin
				.getDefault()
				.getPreferenceStore()
				.setValue(
						DashboardPreferenceConstants.P_DASHBOARD_EXAMPLES_DIR,
						dirText.getStringValue());
		DashboardPlugin
				.getDefault()
				.getPreferenceStore()
				.setValue(DashboardPreferenceConstants.P_DASHBOARD_SCRIPT,
						scriptText.getStringValue());
		super.okPressed();
	}

