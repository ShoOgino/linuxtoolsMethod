    public boolean isPrefixOperator() {
        switch (type) {
            case tPLUS:
            case tMINUS:
            case tSTAR:
            case tAMPER:
            case tCOMPL:
            case tNOT:
            case tINCR:
            case tDECR:
                return true;
            default:
                return false;
        }
    }

