    public boolean looksLikeExpressionStart() {
        switch (type) {
            case tINTEGER:
            case t_false:
            case t_true:
            case tSTRING:
            case tLSTRING:
            case tFLOATINGPT:
            case tCHAR:
            case tAMPER:
            case tDOT:
            case tLPAREN:
                return true;
            default:
                break;
        }
        return false;
    }

