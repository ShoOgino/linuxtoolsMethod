    public boolean looksLikeExpressionEnd() {
        switch (type) {
            case tINTEGER:
            case tSTRING:
            case tLSTRING:
            case tFLOATINGPT:
            case tCHAR:
            case tRPAREN:
            case tIDENTIFIER:
                return true;
            default:
                break;
        }
        return false;
    }

