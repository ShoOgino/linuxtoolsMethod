    private boolean tryEditorSave() {
        if(null == ed) {
            return false;
        }

        if(ed.isDirty()) {
            Display.getDefault().syncExec(new Runnable() {
                @Override
                public void run() {
                    ed.doSave(new ProgressMonitorPart(PlatformUI.getWorkbench().getActiveWorkbenchWindow().getShell(), new FillLayout()));
                }
            });
        }

        return true;
    }

