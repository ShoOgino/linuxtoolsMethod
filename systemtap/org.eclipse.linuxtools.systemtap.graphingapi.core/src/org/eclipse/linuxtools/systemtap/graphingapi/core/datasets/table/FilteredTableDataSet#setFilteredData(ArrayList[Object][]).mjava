	private void setFilteredData(ArrayList<Object>[] data) {
		this.data = new ArrayList<>();

		TableEntry entry = new TableEntry();
		Object[] row;
		for(int j,i=0; i<data[0].size(); i++) {
			row = new Object[data.length];
			for(j=0; j<data.length; j++) {
				row[j] = data[j].get(i);
			}
			entry.add(row);
		}
		this.data.add(entry);
	}

