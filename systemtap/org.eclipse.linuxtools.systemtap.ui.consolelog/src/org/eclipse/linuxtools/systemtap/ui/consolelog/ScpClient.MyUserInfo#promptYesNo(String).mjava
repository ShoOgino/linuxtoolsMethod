		@Override
		public boolean promptYesNo(String str) {
			Object[] options = { "yes", "no" }; //$NON-NLS-1$ //$NON-NLS-2$
			int foo = JOptionPane.showOptionDialog(null, str,
					"Warning", //$NON-NLS-1$
					JOptionPane.DEFAULT_OPTION, JOptionPane.WARNING_MESSAGE,
					null, options, options[0]);
			return foo == 0;
		}

