    private void internalRun(IErrorParser errorParser) {
        if (hasStarted()) {
            reset();
        }
        if (errorParser != null) {
            createErrorDaemon(errorParser);
        }
        createConsoleDaemon();
        notifyConsoleObservers();
        activate();

        onCmdStartThread = new Thread(onCmdStart);
        onCmdStartThread.start();
    }

