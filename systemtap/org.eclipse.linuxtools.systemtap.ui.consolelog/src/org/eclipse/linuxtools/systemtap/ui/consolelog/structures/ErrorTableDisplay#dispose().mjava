	/**
	 * Disposes of all internal references in the class. No method should be called after this.
	 */
	public void dispose() {
		if(null != table && !table.isDisposed()) {
			table.removeMouseListener(mouseListener);
			table.dispose();
			table = null;
		}

		if(titles != null) {
			for(int i=0; i<titles.length; i++) {
				titles[i] = null;
			}
		}
		titles = null;

		if(null != item) {
			item.dispose();
		}
		item = null;
	}

