    private String getModuleName() {
        if (moduleName == null) {
            moduleName = getName();
            int lastSlash = moduleName.lastIndexOf('/')+1;
            if (lastSlash < 0) {
                lastSlash = 0;
            }
            int lastDot = moduleName.indexOf(".stp"); //$NON-NLS-1$
            if (lastDot > 0) {
                moduleName = moduleName.substring(lastSlash, lastDot);
            }
        }
        return moduleName;
    }

