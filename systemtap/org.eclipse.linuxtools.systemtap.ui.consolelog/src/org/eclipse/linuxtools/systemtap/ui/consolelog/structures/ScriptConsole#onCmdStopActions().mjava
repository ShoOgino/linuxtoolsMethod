    private void onCmdStopActions() {
        notifyConsoleObservers();
        final String name = super.getName();
        Display.getDefault().asyncExec(new Runnable() {
            @Override
            public void run() {
                setName(Localization.getString("ScriptConsole.Terminated") + name); //$NON-NLS-1$
            }
        });
    }

