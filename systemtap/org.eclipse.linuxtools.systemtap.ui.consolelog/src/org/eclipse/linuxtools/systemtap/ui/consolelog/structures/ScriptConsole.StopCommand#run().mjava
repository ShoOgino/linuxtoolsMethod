        @Override
        public void run() {
            if (consoleDaemon != null) {
                stopcmd.removeInputStreamListener(consoleDaemon);
            }
            if (errorDaemon != null) {
                stopcmd.removeErrorStreamListener(errorDaemon);
            }
            if (!disposeOnStop) {
                stopcmd.stop();
            } else {
                stopcmd.dispose();
            }
        }

