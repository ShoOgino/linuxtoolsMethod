	public static synchronized ClientSession getInstance () {
		// synchronized so everybody has the same session
		if (instance == null)
			instance = new ClientSession ();
		else
		{
			if(!isConnected())
			{
				instance.connected=instance.createConnection();
		    	instance.scriptnumber = 15;
		    	if (instance.connected) instance.start();
			}
		}
		return instance;
	}

