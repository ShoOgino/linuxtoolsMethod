	public ScpExec(String cmd[], String moduleName) {
		this.moduleName = moduleName;
		this.command = new String();
		
		try{
			
		this.command = cmd[0];
		
		for (int i = 1; i<cmd.length; i++)
		{
			this.command = this.command + " " + cmd[i];
		}
		
		
		}catch(Exception e)
		{
			e.printStackTrace();
		}
		
	}

