    /**
     * @since 3.0
     */
    public ScpExec(String cmds[], RemoteScriptOptions remoteOptions) {
        super(cmds, null);
        this.command = ""; //$NON-NLS-1$
        for (String cmd:cmds) {
            this.command = this.command + " " + cmd; //$NON-NLS-1$
        }
        this.remoteOptions = remoteOptions;
    }

