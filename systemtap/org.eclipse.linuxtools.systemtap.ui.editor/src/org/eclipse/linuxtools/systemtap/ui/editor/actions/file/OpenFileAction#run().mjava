    /**
     * Queries the user for a Systemtap file to open, and opens that file's editor input.
     */
    @Override
    public void run() {
        if (window == null) {
            window = PlatformUI.getWorkbench().getActiveWorkbenchWindow();
        }
        File file = queryFile();
        if (file != null) {
            runActions(file);
        }
    }

