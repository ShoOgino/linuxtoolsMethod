	public static void setValues(Composite graphC, Composite treeC, StapGraph g, StapGraphParser p){
		if (graph != null) {
			graph.dispose();
		}
		
		if (graphComp != null) {
			graphComp.dispose();
		}
		
		if (treeComp != null) {
			treeComp.dispose();
		}
		
		graphComp = graphC;
		treeComp = treeC;
		graph = g;
		parser = p;
	}

