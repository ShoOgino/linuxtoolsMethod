	public  void disposeGraph() {
		if (graphComp != null && !graphComp.isDisposed())
			graphComp.dispose();
		if (treeComp != null && !treeComp.isDisposed())
			treeComp.dispose();
		this.setGraphOptions(false);
		//Force a redraw (.redraw() .update() not working)
		this.maximizeOrRefresh(false);
	}

