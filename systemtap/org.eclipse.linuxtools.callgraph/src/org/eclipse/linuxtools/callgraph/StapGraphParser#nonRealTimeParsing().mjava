		neighbourMaps.clear();
		timeMap.clear();
		serialMap.clear();
		aggregateTimeMap.clear();
		countMap.clear();
		text = ""; //$NON-NLS-1$
		callOrderList.clear();
		shouldGetEndingTimeForID.clear();
		nameMaps.clear();
		idMaps.clear();
