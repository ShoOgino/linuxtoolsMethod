	public static RowDataSet buildDataSet(int rows, int cols) {
		String[] titles = new String[cols];
		int i;
		for(i=0; i<cols; i++)
			titles[i] = ""+ i;
		
		RowDataSet data = new RowDataSet(titles);
		RowEntry entry;
		
		int j;
		for(i=0; i<rows; i++) {
			Object[] d = new Object[cols];
			for(j=0; j< cols; j++) {
				d[j] = new Double(i*cols + j);
			}
			entry = new RowEntry();
			entry.putRow(0, d);
			data.append(entry);
		}
		return data;
	}

