	public static final IDataAggregate createAggregate(String id) {
		switch(getIndex(id)) {
			case 0:
				return new AverageAggregate();
			case 1:
				return new CountAggregate();
			case 2:
				return new MaxAggregate();
			case 3:
				return new MinAggregate();
			case 4:
				return new SumAggregate();
		}
		return null;
	}

