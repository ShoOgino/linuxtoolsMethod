	private boolean isSeriesUnique() {
		Combo item = cboXItem;
		int i = 0;
		do {
			if (item.isVisible() &&
					item.getForeground().equals(item.getDisplay().getSystemColor(SWT.COLOR_RED))) {
				return false;
			}
			if (i == cboYItems.length) {
				return true;
			}
			item = cboYItems[i++];
		} while (true);
	}

