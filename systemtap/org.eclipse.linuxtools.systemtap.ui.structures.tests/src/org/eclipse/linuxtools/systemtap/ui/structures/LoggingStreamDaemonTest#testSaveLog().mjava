	public void testSaveLog() {
		File f = new File("/tmp/loggingstreamdaemon.test");
		assertTrue(daemon.saveLog(f));
		f.delete();

		daemon.handleDataEvent("test");
		assertTrue(daemon.saveLog(f));
		//assertTrue("test".equals(daemon.getOutput()));
		f.delete();
		
		f = new File("/root/");
		assertFalse(daemon.saveLog(f));
		f.delete();
	}

