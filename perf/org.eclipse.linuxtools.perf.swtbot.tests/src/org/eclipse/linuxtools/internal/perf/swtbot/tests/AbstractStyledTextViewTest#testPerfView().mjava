	@Override
	protected void testPerfView() {
		SWTWorkbenchBot bot = new SWTWorkbenchBot();
		SWTBotView view = bot.viewByTitle(getViewId());
		assertNotNull(view);

		view.setFocus();
		SWTBotStyledText text = bot.styledText();
		assertNotNull(text);
		assertEquals(getExpectedText(), text.getText());
	}

