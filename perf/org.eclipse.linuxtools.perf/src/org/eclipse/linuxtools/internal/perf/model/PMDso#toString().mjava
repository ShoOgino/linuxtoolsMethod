	@Override
	public String toString() {
		String prefix = "";
		if (getPercent() != -1)
			prefix += getPercent() + "% (" + getFormattedSamples() + " samples) in ";
		if (kernel == true)
			prefix += "[k] ";
		if (path != null) {
			return prefix + getName() + " (at " + path + ")";
		}
		return prefix + getName();
	}

