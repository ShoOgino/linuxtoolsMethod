	/*
	 * (non-Javadoc)
	 * @see com.st.visualprof.stgprof.view.histogram.TreeElement#getCalls()
	 */
	public int getCalls() {
		if (calls == -1) {
			calls = 0;
			for (TreeElement elem : getChildren()) {
				int i = elem.getCalls();
				if (i != -1) calls += elem.getCalls();
			}
		}
		return calls;
	}

