	/**
	 * Print on System.out, for debugging purpose
	 */
	public void print() {
		System.out.println(this.symbol.getName());
		System.out.println(" -- parents --");
		for (CallGraphArc arc : this.parents) {
			arc.print();
		}
		System.out.println(" -- children --");
		for (CallGraphArc arc : this.children) {
			arc.print();
		}
	}

