	@Override
	public String getToolTipText(Object element) {
		TreeElement elem = (TreeElement) element;
		String s = elem.getSourcePath();
		if (s != null && !"??".equals(s)) {
			int lineNumber = elem.getSourceLine();
			if (lineNumber > 0) {
				return s + ":" + lineNumber;
			}
			return s;
		}
		return null;
	}

