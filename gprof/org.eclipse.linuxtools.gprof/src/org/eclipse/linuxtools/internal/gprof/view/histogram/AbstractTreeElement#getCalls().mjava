	/*
	 * (non-Javadoc)
	 * @see org.eclipse.linuxtools.internal.gprof.view.histogram.TreeElement#getCalls()
	 */
	@Override
	public int getCalls() {
		if (calls == -1) {
			calls = 0;
			for (TreeElement elem : getChildren()) {
				int i = elem.getCalls();
				if (i != -1) calls += elem.getCalls();
			}
		}
		return calls;
	}

