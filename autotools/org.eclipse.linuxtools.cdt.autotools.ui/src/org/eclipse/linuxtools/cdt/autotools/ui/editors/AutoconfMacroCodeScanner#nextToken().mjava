	/*
	 * @see ITokenScanner#nextToken()
	 */
	public IToken nextToken() {
		int ch = read();
		if (ch == '[') {
			incQuoteLevel();
			return getToken(ColorManager.AUTOCONF_DEFAULT_COLOR);
		} else if (ch == ']') {
			decQuoteLevel();
			return getToken(ColorManager.AUTOCONF_DEFAULT_COLOR);
		}
		unread();
		return super.nextToken();
	}

