	public String getCleanBuildTarget() {
		String target = null;
		try {
			target = getProject().getPersistentProperty(AutotoolsPropertyConstants.CLEAN_MAKE_TARGET);
		} catch (CoreException ce) {
			// do nothing
		}
		if (target == null)
			target = AutotoolsPropertyConstants.CLEAN_MAKE_TARGET_DEFAULT;
		return target;
	}

