	public void testComplex2() {
		String text = 
			"if true; then\n" + 
			"AC_CANONICAL_HOST\n" + 
			"else\n" + 
			"case foo in \n" + 
			"	3) 1 ;;\n" + 
			"esac;\n" + 
			"fi\n";
		AutoconfElement tree = parse(text, false);
		assertTreeStructure(tree, new String[] {
				"if",
					"AC_CANONICAL_HOST",
					"else",
						"case",
							"3",
						null,
					null,
				null
		});
	}

