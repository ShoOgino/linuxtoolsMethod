	public void testIfElif() {
		String text = "# comment\n"+
		"\tif true; then\n" +
		"\t\tAC_ONE(...);\n"+
		"\telif false; then \n"+
		"\t\tAC_TWO(...);\n"+
		"\tfi\n";
		AutoconfElement tree = parse(text);
		assertTreeStructure(tree, new String[] { 
				"if", 
					"AC_ONE", 
						"...", 
					null, 
					"elif", 
						"AC_TWO", 
							"...", 
						null, 
					null, 
				null });
	}

