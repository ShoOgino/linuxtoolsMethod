	public void testSelect() {
		String text = 
			"select VAR in 1 2 3; do AC_SOMETHING(...); done\n"+
			"select VAR; do AC_SOMETHING; done\n";
			
		AutoconfElement tree = parse(text, false);
		assertTreeStructure(tree, new String[] { 
				"select", 
					"AC_SOMETHING", 
						"...", 
					null, 
				null,
				"select", 
					"AC_SOMETHING", 
				null
		});
	}

