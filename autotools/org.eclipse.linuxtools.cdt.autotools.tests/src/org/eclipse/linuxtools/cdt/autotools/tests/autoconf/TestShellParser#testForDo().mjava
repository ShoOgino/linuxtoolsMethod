	public void testForDo() {
		// don't get upset by parentheses
		String text = 
			"for (( AC_1; AC_2(); AC_3(...) )); do echo $VAL; done\n";
		AutoconfElement tree = parse(text, false);
		
		assertTreeStructure(tree, new String[] { "for", "AC_1", "AC_2", "", null, "AC_3", "...", null, null });
		
	}

