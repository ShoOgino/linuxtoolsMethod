	/**
	 * Opens the hyperlink in new editor window.
	 */
	@Override
	public void open() {
		IWorkbench ws = PlatformUI.getWorkbench();
		try {
			org.eclipse.ui.ide.IDE.openEditor(ws.getActiveWorkbenchWindow()
					.getActivePage(), fileLoc, true);
		} catch (PartInitException e) {
			e.printStackTrace();

		}

	}

