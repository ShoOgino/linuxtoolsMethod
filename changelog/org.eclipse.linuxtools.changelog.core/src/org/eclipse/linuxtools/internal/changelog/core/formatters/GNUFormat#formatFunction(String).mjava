    private String formatFunction(String function) {

        // If Function Guess is true, and Function Guess has found something
        if (function.length() > 0) {
            return "(" + function + "):"; // $NON-NLS-1$ // $NON-NLS-2$
        } else {
            return ": "; //$NON-NLS-1$
        }
    }

