	/**
	 * Open the SVNRepositoriesView
	 */
	public SVNReporsitoriesView open() {
		bot.menu("Window").menu("Show View").menu("Other...").click();
		SWTBotShell shell = bot.shell("Show View");
		shell.activate();
		bot.tree().expandNode("SVN").select("SVN Repositories");
		bot.button("OK").click();
		return this;
	}

