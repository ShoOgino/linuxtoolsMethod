	public static void buildArrayFromHash(JsonWriter out, String id, Set<String> setNew, boolean asInteger)
			throws IOException {
		if (!setNew.isEmpty()) {
			out.name(id).beginArray();
			for (String string : setNew) {
				if (!"".equals(string)) {
					if (asInteger) {
						out.value(Integer.parseInt(string));
					} else {
						out.value(string);
					}
				}
			}
			out.endArray();
		}
	}

