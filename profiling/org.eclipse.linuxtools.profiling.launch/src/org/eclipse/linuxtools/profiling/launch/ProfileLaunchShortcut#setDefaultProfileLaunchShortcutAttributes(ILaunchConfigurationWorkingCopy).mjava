    /**
     * Set default attributes for the given configuration.
     *
     * @param wc
     * @since 1.2
     */
    public void setDefaultProfileLaunchShortcutAttributes(ILaunchConfigurationWorkingCopy wc){
        try {
            setDefaultProfileAttributes(wc);
        } catch (CoreException e) {
            e.printStackTrace();
        }
    }

