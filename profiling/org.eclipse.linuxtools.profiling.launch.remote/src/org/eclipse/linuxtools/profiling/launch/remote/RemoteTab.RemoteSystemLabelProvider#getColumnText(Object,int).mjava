		public String getColumnText(Object element, int columnIndex) {
			String text = null;
			IHost host = (IHost) element;
			switch (columnIndex) {
			case 0:
				text = host.getName();
				break;
			case 1:
				text = host.getHostName();
				break;
			case 2:
				text = host.getSystemType().getLabel();
				break;
			}
			return text;
		}

