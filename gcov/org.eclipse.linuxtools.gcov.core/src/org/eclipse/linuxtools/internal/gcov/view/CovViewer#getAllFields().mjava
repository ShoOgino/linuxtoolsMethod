	@Override
	public ISTDataViewersField[] getAllFields() {
		if (fields == null) {
			fields = new ISTDataViewersField[] {
					new FieldName(),
					new FieldTotalLines(),
					new FieldInstrumentedLines(),
					new FieldExecutedLines(),
					new FieldCoveragePercentage() };
		}
		return fields;
	}

