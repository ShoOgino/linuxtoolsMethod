    @Override
    public void dispose() {
        if (GREEN_MAX != null) {
            GREEN_MAX.dispose();
        }
        if (GREEN_MIN != null) {
            GREEN_MIN.dispose();
        }
        if (RED != null) {
            RED.dispose();
        }
        for (Color c : GREENCOLORS) {
            if (c != null) {
                c.dispose();
            }
        }
    }

