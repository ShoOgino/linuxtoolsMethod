	/*
	 * (non-Javadoc)
	 * @see org.eclipse.linuxtools.dataviewers.abstractviewers.ISTDataViewersField#getValue(java.lang.Object)
	 */
	@Override
	public String getValue(Object obj) {
		TreeElement e = (TreeElement) obj;
		String nm = e.getName();
		nm = nm.substring(nm.lastIndexOf('/')+1);
		nm = nm.substring(nm.lastIndexOf('\\')+1);
		return nm;
	}

