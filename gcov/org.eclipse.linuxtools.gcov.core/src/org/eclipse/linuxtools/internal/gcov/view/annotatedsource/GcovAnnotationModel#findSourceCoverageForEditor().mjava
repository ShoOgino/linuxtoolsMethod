	private SourceFile findSourceCoverageForEditor() {
		if (editor.isDirty()) {
			return null;
		}
		final IEditorInput input = editor.getEditorInput();
		if (input == null) {
			return null;
		}
		ICElement element = CDTUITools.getEditorInputCElement(input);
		if (element == null) {
			return null;
		}
		return findSourceCoverageForElement(element);
	}

