    /**
     * Standard Constructor.
     *
     * @param callsite
     *              - a CTF call site
     */
    CtfTmfCallsite(CTFCallsite callsite) {
        super(callsite.getFileName(), callsite.getFunctionName(), callsite.getLineNumber());
        fEventName = callsite.getEventName();
        fInstructionPointer = callsite.getIp();
    }

