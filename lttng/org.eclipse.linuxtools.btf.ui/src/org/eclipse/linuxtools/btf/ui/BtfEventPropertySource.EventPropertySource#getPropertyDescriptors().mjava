        @Override
        public IPropertyDescriptor[] getPropertyDescriptors() {
            if (fNote == null) {
                return new IPropertyDescriptor[] {
                new ReadOnlyTextPropertyDescriptor(DESCRIPTION, DESCRIPTION)
                };
            }
            return new IPropertyDescriptor[] {
                    new ReadOnlyTextPropertyDescriptor(DESCRIPTION, DESCRIPTION),
                    new ReadOnlyTextPropertyDescriptor(NOTE, NOTE)
            };
        }

