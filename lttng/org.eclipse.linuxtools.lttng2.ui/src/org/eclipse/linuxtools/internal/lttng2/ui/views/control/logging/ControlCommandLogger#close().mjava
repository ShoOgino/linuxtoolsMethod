    /**
     * Closes the log file if open.
     */
    public static void close() {
        if (fTraceLog == null)
            return;

        try {
            fTraceLog.close();
            fTraceLog = null;
        } catch (IOException e) {
            Activator.getDefault().logWarning("Can't close log file of the trace control", e); //$NON-NLS-1$
        }
    }

