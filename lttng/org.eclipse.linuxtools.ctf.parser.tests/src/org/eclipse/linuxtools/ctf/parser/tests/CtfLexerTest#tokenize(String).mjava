    private void tokenize(String content) {
        CharStream cs = new ANTLRStringStream(content);
        CTFLexer lexer = new CTFLexer(cs);

        tokens.clear();
        for (;;) {
          Token token = lexer.nextToken();
          if (token == Token.EOF_TOKEN) {
            return;
          }
          tokens.add(token);
        }
    }

