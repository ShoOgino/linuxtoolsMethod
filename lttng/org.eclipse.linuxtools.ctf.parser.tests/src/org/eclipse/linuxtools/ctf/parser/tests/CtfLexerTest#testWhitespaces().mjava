    /**
     *  Validate spaces parsing
     */
    @Test
    public void testWhitespaces() {
        tokenize("  \r\t\n\u000C ");
        checkToken(CTFLexer.WS, " ");
        checkToken(CTFLexer.WS, " ");
        checkToken(CTFLexer.WS, "\r");
        checkToken(CTFLexer.WS, "\t");
        checkToken(CTFLexer.WS, "\n");
        checkToken(CTFLexer.WS, "\u000C");
        checkToken(CTFLexer.WS, " ");
    }

