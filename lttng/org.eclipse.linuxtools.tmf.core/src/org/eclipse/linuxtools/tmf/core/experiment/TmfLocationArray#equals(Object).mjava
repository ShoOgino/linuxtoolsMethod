    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (obj == null)
            return false;
        if (getClass() != obj.getClass())
            return false;
        TmfLocationArray other = (TmfLocationArray) obj;
        if (!Arrays.equals(locations, other.locations))
            return false;
        return true;
    }

