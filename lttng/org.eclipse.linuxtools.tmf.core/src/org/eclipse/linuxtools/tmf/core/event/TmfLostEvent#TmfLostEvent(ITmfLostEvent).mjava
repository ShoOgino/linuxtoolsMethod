    /**
     * Copy constructor
     *
     * @param event the original event
     */
    public TmfLostEvent(final ITmfLostEvent event) {
        super(  event.getTrace(),
                event.getRank(),
                event.getTimestamp(),
                event.getSource(),
                event.getType(),
                event.getContent(),
                event.getReference());
        fTimeRange = event.getTimeRange();
        fNbLostEvents = event.getNbLostEvents();
    }

