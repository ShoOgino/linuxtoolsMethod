    /**
     * @since 3.0
     */
    @Override
    public <T extends IAnalysisModule> T getAnalysisModuleOfClass(Class<T> moduleClass, String id) {
        Iterable<T> modules = getAnalysisModulesOfClass(moduleClass);
        for (T module : modules) {
            if (id.equals(module.getId())) {
                return module;
            }
        }
        return null;
    }

