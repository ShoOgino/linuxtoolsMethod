    /**
     * Method seekEvent.
     * @param timestamp ITmfTimestamp
     * @return ITmfContext
     * @see org.eclipse.linuxtools.tmf.core.trace.ITmfTrace#seekEvent(ITmfTimestamp)
     */
    @Override
    public ITmfContext seekEvent(final ITmfTimestamp timestamp) {
        iterator.seek(timestamp.getValue());
        return iterator;
    }

