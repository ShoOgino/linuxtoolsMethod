    /**
     * Method getType.
     * @return ITmfEventType
     * @see org.eclipse.linuxtools.tmf.core.event.ITmfEvent#getType()
     */
    @Override
    public ITmfEventType getType() {
        if(type == null){
            type = new CtfTmfEventType(CONTEXT_ID, eventName, fContent);
        }
        return type;
    }

