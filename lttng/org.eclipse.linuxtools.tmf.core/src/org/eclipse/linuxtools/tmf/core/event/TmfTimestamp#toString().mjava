    /* (non-Javadoc)
     * @see java.lang.Object#toString()
     */
    @Override
    @SuppressWarnings("nls")
    public String toString() {
        if (equals(BIG_BANG)) {
            return "BIG_BANG";
        }
        if (equals(BIG_CRUNCH)) {
            return "BIG_CRUNCH";
        }
        if (equals(ZERO)) {
            return "ZERO";
        }
        return "[" + fValue + "," + fScale + "," + fPrecision + "]";
    }

