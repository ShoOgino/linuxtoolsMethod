    /* (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.core.request.ITmfRequest#fail()
     */
    @Override
    public void fail() {
        synchronized (this) {
            if (fRequestStatus == null) {
                fRequestStatus = new Status(IStatus.ERROR, PLUGIN_ID, "FAILED"); //$NON-NLS-1$
            }
        }
        done();
    }

