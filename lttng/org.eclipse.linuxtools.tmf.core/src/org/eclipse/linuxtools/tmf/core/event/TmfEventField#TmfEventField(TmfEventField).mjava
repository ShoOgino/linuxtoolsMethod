    /**
     * Copy constructor
     * 
     * @param field the other event field
     */
    public TmfEventField(TmfEventField field) {
    	if (field == null)
    		throw new IllegalArgumentException();
    	fEventContent  = field.fEventContent;
    	fFieldId = field.fFieldId;
		fValue = field.fValue;
		fSubFields = field.fSubFields;
    }

