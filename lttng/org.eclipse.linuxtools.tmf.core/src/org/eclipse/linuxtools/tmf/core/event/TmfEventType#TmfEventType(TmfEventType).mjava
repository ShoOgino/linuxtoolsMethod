	/**
	 * Copy constructor
	 * @param other
	 */
	public TmfEventType(TmfEventType other) {
    	if (other == null)
    		throw new IllegalArgumentException();
		fTypeId      = other.fTypeId;
		fFieldLabels = other.fFieldLabels;
		fNbFields    = other.fNbFields;
		fFieldMap    = other.fFieldMap;
	}

