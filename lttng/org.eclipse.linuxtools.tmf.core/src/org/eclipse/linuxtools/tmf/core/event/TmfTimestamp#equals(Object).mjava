    /* (non-Javadoc)
     * @see java.lang.Object#equals(java.lang.Object)
     */
    @Override
    public boolean equals(Object other) {
        if (this == other)
            return true;
        if (other == null)
            return false;
        if (!(other instanceof TmfTimestamp))
            return false;
        TmfTimestamp ts = (TmfTimestamp) other;
        return compareTo(ts, false) == 0;
    }

