	@Override
	public boolean matches(TmfEvent event) {
        Object value = event.getContent().getField(fField);
        if (value == null) {
            return false ^ fNot;
        }
        String valueString = value.toString();
        if (valueString == null) {
            return false ^ fNot;
        }
        if (fIgnoreCase) {
            return valueString.equalsIgnoreCase(fValue) ^ fNot;
        } else {
            return valueString.equals(fValue) ^ fNot;
        }
	}

