    /* (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.core.request.ITmfRequest#cancel()
     */
    @Override
    public void cancel() {
        synchronized (this) {
            if (fRequestStatus == null) {
                fRequestStatus = new Status(IStatus.CANCEL, PLUGIN_ID, "CANCEL"); //$NON-NLS-1$
            }
        }
        done();
    }

