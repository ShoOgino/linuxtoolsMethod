    /* (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.core.event.ITmfEventField#getFieldName(int)
     */
    @Override
    public String getFieldName(final int index) {
        final ITmfEventField field = getField(index);
        if (field != null)
            return field.getName();
        return null;
    }

