    /**
     * Seek rank ratio
     * @param ratio double
     * @return ITmfContext
     * @see org.eclipse.linuxtools.tmf.core.trace.ITmfTrace#seekEvent(double)
     */
    @Override
    public ITmfContext seekEvent(final double ratio) {
        iterator.seek((long) (this.fNbEvents * ratio));
        return iterator;
    }

