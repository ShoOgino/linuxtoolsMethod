    /*
     * (non-Javadoc)
     *
     * @see java.lang.Object#toString()
     */
    @Override
    public String toString() {
        StringBuilder retVal = new StringBuilder();
        for( ITmfEventField field : getFields())        {
            retVal.append(field.toString());
            retVal.append('\t');
        }
        return retVal.toString();
    }

