    /* (non-Javadoc)
     * @see java.util.Iterator#next()
     */
    @Override
    public ITmfEvent next() {
        ITmfEvent event;
        if (fNextEvent != null) {
            event = fNextEvent;
            fNextEvent = null;
        } else {
            event = fTrace.getNext(fContext);
        }
        return event;
    }

