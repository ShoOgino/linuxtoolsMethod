	@Override
	public boolean matches(TmfEvent event) {
        if (fPattern == null) {
            return false ^ fNot;
        }
        Object value = event.getContent().getField(fField);
        if (value == null) {
            return false ^ fNot;
        }
        String valueString = value.toString();
        return fPattern.matcher(valueString).matches() ^ fNot;
	}

