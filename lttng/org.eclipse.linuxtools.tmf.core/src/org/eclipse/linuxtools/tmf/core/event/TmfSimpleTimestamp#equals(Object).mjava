    @Override
    public boolean equals(Object other) {
        if (this == other)
            return true;
        if (other == null)
            return false;
        if (!(other instanceof TmfSimpleTimestamp))
            return super.equals(other);
        TmfSimpleTimestamp ts = (TmfSimpleTimestamp) other;
        return compareTo(ts, false) == 0;
    }

