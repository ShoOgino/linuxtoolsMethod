    /**
     * @since 2.0
     */
    @Override
    public long unboxLong() throws StateValueTypeException {
        if (this.isNull()) {
            /* Long value expected, return "-1" instead */
            return -1;
        }

        if (this.getType() != Type.LONG) {
            throw new StateValueTypeException();
        }
        return (Long) this.getValue();
    }

