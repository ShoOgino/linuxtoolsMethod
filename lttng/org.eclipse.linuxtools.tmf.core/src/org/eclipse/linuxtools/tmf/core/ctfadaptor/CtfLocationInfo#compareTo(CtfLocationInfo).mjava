    @Override
    public int compareTo(CtfLocationInfo other) {
        if (fTimestamp > other.getTimestamp()) {
            return 1;
        }
        if (fTimestamp < other.getTimestamp()) {
            return -1;
        }
        if (fIndex > other.getIndex()) {
            return 1;
        }
        if (fIndex < other.getIndex()) {
            return -1;
        }
        return 0;
    }

