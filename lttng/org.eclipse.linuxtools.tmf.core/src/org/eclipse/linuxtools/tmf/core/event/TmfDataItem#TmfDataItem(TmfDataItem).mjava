    /**
     * Copy constructor
     * 
     * @param event the original event
     */
    public TmfDataItem(TmfDataItem event) {
        if (event == null)
            throw new IllegalArgumentException();
        fTrace = event.fTrace;
        fRank = event.fRank;
        fSource = event.fSource;
        fType = event.fType;
        fContent = event.fContent;
        fReference = event.fReference;
    }

