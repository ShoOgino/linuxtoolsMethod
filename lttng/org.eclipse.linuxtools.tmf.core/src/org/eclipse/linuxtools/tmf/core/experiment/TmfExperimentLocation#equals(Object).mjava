    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (!super.equals(obj))
            return false;
        if (getClass() != obj.getClass())
            return false;
        TmfExperimentLocation other = (TmfExperimentLocation) obj;
        if (!Arrays.equals(fRanks, other.fRanks))
            return false;
        return true;
    }

