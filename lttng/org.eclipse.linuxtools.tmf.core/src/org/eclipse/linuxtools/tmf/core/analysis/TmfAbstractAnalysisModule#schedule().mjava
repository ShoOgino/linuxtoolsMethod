    @Override
    public IStatus schedule() {
        final ITmfTrace trace = fTrace;
        if (trace == null) {
            return new Status(IStatus.ERROR, Activator.PLUGIN_ID, String.format("No trace specified for analysis %s", getName())); //$NON-NLS-1$
        }
        execute(trace);

        return Status.OK_STATUS;
    }

