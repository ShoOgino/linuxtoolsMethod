    @Override
    public TmfCheckpoint clone() {
        TmfCheckpoint result = null;
        try {
            result = (TmfCheckpoint) super.clone();
            result.fTimestamp = fTimestamp.clone();
            result.fLocation = fLocation.clone();
            return result;
        } catch (final CloneNotSupportedException e) {
            e.printStackTrace();
        }
        return result;
    }

