    @SuppressWarnings("unchecked")
    public void insertSorted(T value) {
        add(value);
        Comparable<T> cmp = (Comparable<T>) value;
        for (int pos = size() - 1; pos > 0 && cmp.compareTo(get(pos - 1)) < 0; pos--) {
            T tmp = get(pos);
            set(pos, get(pos - 1));
            set(pos - 1, tmp);
        }
    }

