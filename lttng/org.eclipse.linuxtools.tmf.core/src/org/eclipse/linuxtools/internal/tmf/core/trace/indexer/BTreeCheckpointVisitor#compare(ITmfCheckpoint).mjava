    @Override
    public int compare(ITmfCheckpoint currentCheckpoint) {
        int compareTo = currentCheckpoint.compareTo(search);
        if (compareTo <= 0 && !exactFound) {
            rank = currentCheckpoint.getCheckpointRank();
            found = currentCheckpoint;
            if (compareTo == 0) {
                exactFound = true;
            }
        }
        return compareTo;
    }

