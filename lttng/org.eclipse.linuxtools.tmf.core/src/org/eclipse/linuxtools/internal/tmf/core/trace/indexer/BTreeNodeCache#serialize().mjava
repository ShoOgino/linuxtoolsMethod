    /**
     * Write all in-memory nodes to disk if they are dirty
     */
    void serialize() {
        if (fRootNode != null && fRootNode.isDirty()) {
            fRootNode.serializeOut();
        }
        for (BTreeNode nodeSearch : fCachedNodes) {
            if (nodeSearch.isDirty()) {
                nodeSearch.serializeOut();
            }
        }
    }

