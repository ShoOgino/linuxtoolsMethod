    /**
     * Set the root node. See {@link #fRootNode}
     *
     * @param newRootNode
     *            the new root node
     */
    void setRootNode(BTreeNode newRootNode) {
        BTreeNode oldRootNode = fRootNode;
        fRootNode = newRootNode;
        if (oldRootNode != null) {
            addNode(oldRootNode);
        }
        return;
    }

