    /* (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.core.request.TmfRequest#isCompleted()
     */
    @Override
    public synchronized boolean isCompleted() {
        for (ITmfRequest request : fSubRequests) {
            if (!request.isCompleted()) {
                return false;
            }
        }
        return super.isCompleted();
    }

