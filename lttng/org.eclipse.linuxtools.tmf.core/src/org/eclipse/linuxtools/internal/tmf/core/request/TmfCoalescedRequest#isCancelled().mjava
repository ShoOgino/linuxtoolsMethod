    /* (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.core.request.TmfRequest#isCancelled()
     */
    @Override
    public synchronized boolean isCancelled() {
        for (ITmfRequest request : fSubRequests) {
            if (!request.isCancelled()) {
                return false;
            }
        }
        return super.isCancelled();
    }

