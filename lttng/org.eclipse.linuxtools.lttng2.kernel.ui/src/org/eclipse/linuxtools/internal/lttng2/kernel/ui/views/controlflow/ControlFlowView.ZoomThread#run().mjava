        @Override
        public void run() {
            ArrayList<ControlFlowEntry> entryList = fEntryList;
            if (entryList == null) {
                return;
            }
            for (ControlFlowEntry entry : entryList) {
                if (fMonitor.isCanceled()) {
                    return;
                }
                zoom(entry, fMonitor);
            }
            redraw();
        }

