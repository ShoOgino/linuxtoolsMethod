        @Override
        public void run() {
            ArrayList<ControlFlowEntry> entryList = null;
            synchronized (fEntryListSyncObj) {
                entryList = fEntryList;
            }
            if (entryList == null) {
                return;
            }
            for (ControlFlowEntry entry : entryList) {
                if (fMonitor.isCanceled()) {
                    break;
                }
                zoom(entry, fMonitor);
            }
            redraw();
        }

