    @Override
    public int compare(ITmfStateInterval o1, ITmfStateInterval o2) {
        long e1 = o1.getEndTime();
        long e2 = o2.getEndTime();

        if (e1 < e2) {
            return -1;
        } else if (e1 > e2) {
            return 1;
        } else {
            return 0;
        }
    }

