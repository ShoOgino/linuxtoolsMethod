    @Override
    public boolean equals(@Nullable Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (getClass() != obj.getClass()) {
            return false;
        }

        PacketUniqueID other = (PacketUniqueID) obj;
        if (fIndex != other.fIndex) {
            return false;
        }
        if (fPath != other.fPath) {
            return false;
        }
        return true;
    }

