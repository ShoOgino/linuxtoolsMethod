    private String generateFlagString() {
        StringBuilder sb = new StringBuilder();
        boolean start = true;

        if (fDontFragmentFlag) {
            if (!start) {
                sb.append(", "); //$NON-NLS-1$
            }
            sb.append("DF"); //$NON-NLS-1$
            start = false;
        }
        if (fMoreFragmentFlag) {
            if (!start) {
                sb.append(", "); //$NON-NLS-1$
            }
            sb.append("MF"); //$NON-NLS-1$
            start = false;
        }
        String string = sb.toString();
        if (string == null) {
            return EMPTY_STRING;
        }
        return string;
    }

