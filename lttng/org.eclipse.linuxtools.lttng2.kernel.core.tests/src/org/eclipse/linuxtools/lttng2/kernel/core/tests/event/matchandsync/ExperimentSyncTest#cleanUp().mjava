    /**
     * Reset the timestamp transforms on the traces
     */
    @After
    public void cleanUp() {
        fTraces[0].setTimestampTransform(TmfTimestampTransform.IDENTITY);
        fTraces[1].setTimestampTransform(TmfTimestampTransform.IDENTITY);
        fTraces[0].dispose();
        fTraces[1].dispose();
    }

