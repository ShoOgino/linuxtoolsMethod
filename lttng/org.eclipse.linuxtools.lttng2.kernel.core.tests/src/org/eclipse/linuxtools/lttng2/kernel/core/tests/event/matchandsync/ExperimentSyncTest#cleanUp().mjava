    /**
     * Reset the timestamp transforms on the traces
     */
    @After
    public void cleanUp() {
        fTraces[0].setTimestampTransform(TimestampTransformFactory.getDefaultTransform());
        fTraces[1].setTimestampTransform(TimestampTransformFactory.getDefaultTransform());
        fTraces[0].dispose();
        fTraces[1].dispose();
    }

