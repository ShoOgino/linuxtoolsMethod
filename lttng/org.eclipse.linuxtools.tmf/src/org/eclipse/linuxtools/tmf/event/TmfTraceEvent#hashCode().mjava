    @Override
    public int hashCode() {
		int result = super.hashCode();
		result = 37 * result + ((fSourcePath != null) ? fSourcePath.hashCode() : 0);
		result = 37 * result + ((fFileName   != null) ? fFileName.hashCode()   : 0);
		result = 37 * result + fLineNumber;
        return result;
    }

