	/**
	 * @param startTime
	 * @param endTime
	 */
	public TmfTimeRange(TmfTimestamp startTime, TmfTimestamp endTime) {
		if (startTime == null || endTime == null) {
    		throw new IllegalArgumentException();
		}
		fStartTime =  new TmfTimestamp(startTime);
		fEndTime   =  new TmfTimestamp(endTime);
	}

