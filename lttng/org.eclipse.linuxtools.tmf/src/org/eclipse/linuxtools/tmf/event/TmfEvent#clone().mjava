	@Override
	public TmfEvent clone() {
		TmfEvent event = new TmfEvent(
			fOriginalTimestamp.clone(),
			fEffectiveTimestamp.clone(),
		    fSource.clone(),
		    fType.clone(),
		    fReference.clone());
		TmfEventContent content = fContent.clone();
		event.setContent(content);
		return event;
	}

