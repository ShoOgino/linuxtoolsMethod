    @Override
    public boolean equals(Object other) {
    	if (other instanceof TmfDataRequest<?>) {
    		TmfDataRequest<?> request = (TmfDataRequest<?>) other;
    		return 	(request.fDataType    == fDataType) &&
    				(request.fIndex       == fIndex)    &&
    				(request.fNbRequested == fNbRequested);
    	}
    	return false;
    }

