	@Override
	public boolean matches(TmfEvent event) {
		if (event.getType().getTypeId().equals(fName)) {
			// There should be at most one child
			for (ITmfFilterTreeNode node : getChildren()) {
				if (! node.matches(event)) {
					return false;
				}
			}
			return true;
		}
		return false;
	}

