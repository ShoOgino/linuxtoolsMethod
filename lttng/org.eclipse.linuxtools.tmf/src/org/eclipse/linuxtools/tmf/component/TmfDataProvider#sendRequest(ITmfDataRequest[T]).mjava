	public void sendRequest(final ITmfDataRequest<T> request) {
		synchronized(this) {
			if (fClone == null || request.getExecType() == SHORT) {
				if (fSignalDepth > 0) {
					coalesceDataRequest(request);
				} else {
					queueRequest(request);
				}
			}
			else {
				fClone.sendRequest(request);
			}
		}
	}

