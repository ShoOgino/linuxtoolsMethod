	/**
	 * This method queues the coalesced requests.
	 * 
	 * @param thread
	 */
	public void fireRequests() {
		synchronized(this) {
			for (TmfDataRequest<T> request : fPendingCoalescedRequests) {
				queueRequest(request);
			}
			fPendingCoalescedRequests.clear();

			if (fClone != null)
				fClone.fireRequests();
		}
	}

