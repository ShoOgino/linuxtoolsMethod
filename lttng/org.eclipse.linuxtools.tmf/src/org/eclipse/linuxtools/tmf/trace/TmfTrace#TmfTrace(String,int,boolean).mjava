    /**
     * @param name
     * @param pageSize
     * @param index
     * @throws FileNotFoundException
     */
    protected TmfTrace(String path, int pageSize, boolean index) throws FileNotFoundException {
    	int sep = path.lastIndexOf(File.separator);
    	fName = (sep >= 0) ? path.substring(sep + 1) : path;
    	fPath = path;
        fPageSize = pageSize;
        fIndex = index;
    }

