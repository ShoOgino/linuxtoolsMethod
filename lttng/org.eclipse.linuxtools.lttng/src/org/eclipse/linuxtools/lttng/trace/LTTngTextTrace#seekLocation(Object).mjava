    public TmfTraceContext seekLocation(Object location) {

    	if (location == null) {
    		location = 0L;
    	}

    	if (!((Long) location).equals(nbCharRead)) {
    		skipToPosition((Long) location);
    	}

    	TmfTraceContext tmpTraceContext =  new TmfTraceContext(location, 0L);
    	
    	return tmpTraceContext;
    }

