	// ========================================================================
	// Constructor
	// ========================================================================
	LttngStateInputRef(TmfTrace<LttngEvent> log) {
		this.log = log;
		
		if ( log instanceof LTTngTrace) {
			cpuNumber = ((LTTngTrace)log).getCpuNumber();
		}
		else if ( log instanceof LTTngTextTrace) {
			cpuNumber = ((LTTngTextTrace)log).getCpuNumber();
		}
	}

