    @Override
	public String toString() {
    	String returnedData="";
    	
    	returnedData += "Event timestamp:" + this.getTimestamp().getValue() + " ";
    	returnedData += "Channel:" + getChannelName() + " ";
    	returnedData += "CPU:" + getCpuId() + " ";
    	returnedData += "Marker:" + getMarkerName() + " ";
    	
    	return returnedData;
    }

