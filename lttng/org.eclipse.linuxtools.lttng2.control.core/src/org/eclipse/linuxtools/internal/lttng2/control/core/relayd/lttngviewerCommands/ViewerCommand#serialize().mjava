    @Override
    public byte[] serialize() {
        byte data[] = new byte[SIZE];
        ByteBuffer bb = ByteBuffer.wrap(data);
        bb.order(ByteOrder.BIG_ENDIAN);
        bb.putLong(getDataSize());
        bb.putInt(getCmd().getCommand());
        bb.putInt(fCmdVersion);
        return data;
    }

