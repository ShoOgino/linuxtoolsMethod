    /**
     * Fail if it validates
     */
    @Test
    public void validateCompactFail() {
        for (int i = 0; i < declarations.size(); i++) {
            if (i == VALID_COMPACT) {
                continue;
            }
            assertEquals(false, EventHeaderCompactDeclaration.isCompactEventHeader(declarations.get(i)));
        }
    }

