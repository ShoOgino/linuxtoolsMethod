    private static BitBuffer createBuffer(int j) throws CTFReaderException {
        final byte[] bytes = new byte[j];
        for (int i = 0; i < j; i++) {
            bytes[i] = (byte) (i % 0xff);
        }
        BitBuffer fixture = new BitBuffer(ByteBuffer.wrap(bytes));
        fixture.position(1);
        return fixture;
    }

