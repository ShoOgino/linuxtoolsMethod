    /**
     * Fail if it validates
     */
    @Test
    public void validateLargeFail() {
        for (int i = 0; i < declarations.size(); i++) {
            if (i == VALID_LARGE) {
                continue;
            }
            assertEquals(false, EventHeaderLargeDeclaration.isLargeEventHeader(declarations.get(i)));
        }
    }

