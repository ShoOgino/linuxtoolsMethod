    /**
     * Dispose the CTFTraceReader
     *
     * @since 3.0
     */
    @Override
    public void close() {
        for (CTFStreamInputReader reader : fStreamInputReaders) {
            if (reader != null) {
                try {
                    reader.close();
                } catch (IOException e) {
                    Activator.logError(e.getMessage(), e);
                }
            }
        }
        fStreamInputReaders.clear();
    }

