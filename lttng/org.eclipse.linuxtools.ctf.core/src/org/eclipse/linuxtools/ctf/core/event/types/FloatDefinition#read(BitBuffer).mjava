    @Override
    public void read(BitBuffer input) {
        int exp = declaration.getExponent();
        int mant = declaration.getMantissa();
        if( (exp + mant) == 32 ){
            readRawFloat32(input, mant , exp);
        }
        else if((exp + mant) == 64)
        {
            readRawFloat64(input, mant,exp);
        }
        else
        {
            value = Double.NaN;
        }
    }

