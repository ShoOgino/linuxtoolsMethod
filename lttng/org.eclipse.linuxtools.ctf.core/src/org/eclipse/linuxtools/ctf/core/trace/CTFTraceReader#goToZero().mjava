    /**
     * Go to the first entry of a trace
     *
     * @return 0, the first index.
     */
    private long goToZero() {
        long tempIndex;
        for (StreamInputReader streamInputReader : this.streamInputReaders) {
            /*
             * Seek the trace reader.
             */
            streamInputReader.seek(0);
        }
        tempIndex = 0;
        return tempIndex;
    }

