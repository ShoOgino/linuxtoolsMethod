    @Override
    @Nullable
    public LexicalScope getChild(String name) {
        if (name.equals(EVENT_HEADER_ID.getName())) {
            return EVENT_HEADER_ID;
        }
        if (name.equals(EVENT_HEADER_V.getName())) {
            return EVENT_HEADER_V;
        }
        return super.getChild(name);
    }

