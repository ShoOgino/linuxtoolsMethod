    /**
     * Dispose the trace
     *
     * @since 3.0
     */
    @Override
    public void close() {
        for (FileInputStream fis : fFileInputStreams) {
            if (fis != null) {
                try {
                    fis.close();
                } catch (IOException e) {
                    // do nothing it's ok, we tried to close it.
                }
            }
        }
        // Invoke GC to release MappedByteBuffer objects (Java bug JDK-4724038)
        System.gc();
    }

