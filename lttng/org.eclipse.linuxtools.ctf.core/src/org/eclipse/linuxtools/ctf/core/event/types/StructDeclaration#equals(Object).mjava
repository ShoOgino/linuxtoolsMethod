    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (!(obj instanceof StructDeclaration)) {
            return false;
        }
        StructDeclaration other = (StructDeclaration) obj;
        if (!fFieldMap.entrySet().equals(other.fFieldMap.entrySet())) {
            return false;
        }
        if (fMaxAlign != other.fMaxAlign) {
            return false;
        }
        return true;
    }

