    /**
     * Dispose the trace
     * @since 2.0
     */
    public void dispose() {
        for (FileInputStream fis : fileInputStreams) {
            if (fis != null) {
                try {
                    fis.close();
                } catch (IOException e) {
                    // do nothing it's ok, we tried to close it.
                }
            }
        }
        System.gc(); // Invoke GC to release MappedByteBuffer objects (Java bug JDK-4724038)
    }

