    @Override
    protected void finalize() {
        /* If this trace gets closed, release the descriptors to the streams */
        for (FileChannel fc : streamFileChannels) {
            if (fc != null) {
                try {
                    fc.close();
                } catch (IOException e) {
                    // do nothing it's ok, we tried to close it.
                }
            }
        }
        try {
            super.finalize();
        } catch (Throwable e) {
            // TODO Auto-generated catch block
        }
    }

