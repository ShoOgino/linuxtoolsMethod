    private void readQuote(StringBuffer buffer, RandomAccessFile raFile, char eq) {
        try {
            int i;
            while ((i = raFile.read()) != -1) {
                char c = (char)i;
                buffer.append(c);
                if (c == eq) {
                    break; // found matching end-quote
                }
            }
            return;
        } catch (IOException e) {
            return;
        }
    }

