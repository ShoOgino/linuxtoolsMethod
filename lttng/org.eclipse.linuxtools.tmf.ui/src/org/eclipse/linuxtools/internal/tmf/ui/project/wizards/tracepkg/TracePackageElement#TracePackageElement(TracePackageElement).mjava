    /**
     *
     * @param parent
     *            the parent of this element, can be set to null
     */
    public TracePackageElement(TracePackageElement parent) {
        fParent = parent;
        fEnabled = true;
        fVisible = true;
        fChildren = new ArrayList<>();
        if (parent != null) {
            parent.addChild(this);
        }
    }

