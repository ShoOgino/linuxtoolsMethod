	private void calcTimeDelta(int width, double K) {
		long D[] = { 1, 2, 5, };
		long pow = 1;
		double powD = 1;
		long td = pow;
		double tdD = powD;
		double dx = tdD * K;
		int i = 0;
		while (dx < width) {
			td = D[i] * pow;
			tdD = D[i] * powD;
			dx = tdD * K;
			i++;
			if (i == 3) {
				i = 0;
				pow *= 10;
				powD *= 10;
			}
		}
		_timeDeltaD = tdD;
		_timeDelta = td;
//		Trace.debug("Width: " + width + " K: " + K + " Time Delta: "
//				+ _timeDelta);
	}

