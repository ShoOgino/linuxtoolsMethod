    @Override
    public void setLifeline(Lifeline theLifeline) {
        super.setLifeline(theLifeline);
        if (fLifeline != null && fHasTimeInfo) {
            fLifeline.fHasTimeInfo = true;
            if (fLifeline.getFrame() != null) {
                fLifeline.getFrame().setHasTimeInfo(true);
            }
        }
    }

