    /**
     * Sets the trace of this model.
     * @param trace - a {@link ITmfTrace}
     * @since 3.0
     */
    public void setTrace(ITmfTrace trace) {
        this.fTrace = trace;
        fTraceMap.clear();
        ITmfTrace[] traces = TmfTraceManager.getTraceSet(fTrace);
        if (traces != null) {
            int i = 0;
            for (ITmfTrace tr : traces) {
                fTraceMap.put(tr, i);
                i++;
            }
        }
    }

