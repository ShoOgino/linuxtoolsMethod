    /*
     * (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.ui.views.TmfView#dispose()
     */
    @Override
    public void dispose() {
        super.dispose();
        if (fWaitCursor != null) {
            fWaitCursor.dispose();
        }

        // Make sure there is no request running before removing the statistics tree
        cancelOngoingRequest();
        // clean the model
        TmfStatisticsTreeRootFactory.removeAll();
    }

