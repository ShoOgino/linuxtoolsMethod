    @Override
    public int category(Object element) {
        if (element instanceof TmfExperimentFolder) {
            return 0;
        }
        if (element instanceof TmfTraceFolder) {
            return 0;
        }
        if (element instanceof TmfExperimentElement) {
            return 1;
        }
        if (element instanceof TmfTraceElement) {
            return 1;
        }
        return 2;
    }

