    /*
     * (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.ui.views.uml2sd.ScrollView#contentsToViewX(int)
     */
    @Override
    public int contentsToViewX(int _x) {
        if (printing) {
            int v = Math.round(printerX * printerZoom);
            return _x - v;
        }
        return _x - getContentsX();
    }

