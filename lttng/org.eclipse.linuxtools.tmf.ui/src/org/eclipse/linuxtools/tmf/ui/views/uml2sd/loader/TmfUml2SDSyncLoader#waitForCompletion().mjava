    /**
     * Waits for the page request to be completed
     */
    public void waitForCompletion() {
        fLock.lock();
        ITmfEventRequest<ITmfEvent> request = fPageRequest;
        fLock.unlock();
        if (request != null)
        try {
            request.waitForCompletion();
        } catch (InterruptedException e) {
            // ignore
        }
    }

