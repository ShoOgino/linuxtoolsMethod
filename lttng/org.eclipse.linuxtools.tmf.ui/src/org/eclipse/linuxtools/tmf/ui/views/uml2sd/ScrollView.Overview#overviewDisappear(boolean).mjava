        /** Process disappear of overview */
        protected void overviewDisappear(boolean restoreCursorLoc) {
            if (overview == null)
                return;
            overview.setVisible(false);
            corner_control_.setCursor(null);
            if (restoreCursorLoc)
                getDisplay().setCursorLocation(save_cursor_x, save_cursor_y);
            overview.dispose();
            overview = null;
        }

