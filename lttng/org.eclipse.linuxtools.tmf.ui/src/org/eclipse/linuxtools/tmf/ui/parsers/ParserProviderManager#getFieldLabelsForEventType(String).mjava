    public static String[] getFieldLabelsForEventType(String eventType) {
        for (IParserProvider parserProvider : fParserProviders) {
        	String[] fieldLabels = parserProvider.getFieldLabelsForEventType(eventType);
        	if (fieldLabels != null) {
        		return fieldLabels;
        	}
        }
        return new String[0];
    }

