    /*
     * (non-Javadoc)
     *
     * @see org.eclipse.swt.widgets.Widget#dispose()
     */
    @Override
    public void dispose() {
        super.dispose();
        for (ITmfViewer viewer : fViewers.values()) {
            viewer.dispose();
        }
        if (fFolder != null) {
            fFolder.dispose();
        }
    }

