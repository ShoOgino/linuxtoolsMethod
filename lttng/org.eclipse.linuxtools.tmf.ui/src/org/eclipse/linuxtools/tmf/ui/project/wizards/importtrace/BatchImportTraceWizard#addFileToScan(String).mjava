    /**
     * Add a file to scan
     *
     * @param fileName
     *            the file to scan
     */
    public void addFileToScan(final String fileName) {
        if (!fParentFiles.containsKey(fileName)) {
            fParentFiles.put(fileName, new HashSet<String>());
            startUpdateTask(Messages.BatchImportTraceWizard_add + ' ' + fileName, fileName);

        }

    }

