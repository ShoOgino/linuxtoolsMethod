    /*
     * (non-Javadoc)
     *
     * @see org.eclipse.linuxtools.tmf.core.component.TmfComponent#dispose()
     */
    @Override
    public void dispose() {
        if (fWaitCursor != null) {
            fWaitCursor.dispose();
        }
        fTreeViewer.getControl().dispose();
        super.dispose();
        // clean the model
        TmfStatisticsTreeRootFactory.removeAll();
    }

