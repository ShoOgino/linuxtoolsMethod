    @Override
    public void draw(IGC context) {
        super.draw(context);
        int x = getX();
        int y = getY();
        int width = getWidth();
        int height = getHeight();
        if (image != null) {
            context.drawImage(image, x + width - 4, y + height - 11, 8, 11);
        }
        if (ellipsesImage != null) {
            context.drawImage(ellipsesImage, x + width, y, 40, 10);
        }
    }

