	@TmfSignalHandler
    public void traceUpdated(TmfStreamUpdatedSignal signal) {
        // Perform the refresh on the UI thread
    	fTable.getDisplay().asyncExec(new Runnable() {
			public void run() {
				if (!fTable.isDisposed()) {
			    	fTable.setItemCount(fTrace.getNbEvents());        
				}
			}
        });
    }

