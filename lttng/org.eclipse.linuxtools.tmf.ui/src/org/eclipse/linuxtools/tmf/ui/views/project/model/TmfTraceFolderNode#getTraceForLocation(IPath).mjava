    /**
     * @return
     */
	public TmfTraceNode getTraceForLocation(IPath location) {
	    for (ITmfProjectTreeNode node : fChildren) {
	        TmfTraceNode trace = (TmfTraceNode) node;
	        if (trace.getResource().getLocation().equals(location)) {
	            return trace;
	        }
	    }
	    return null;
	}

