    /*
     * (non-Javadoc)
     * @see org.eclipse.jface.action.Action#run()
     */
    @Override
    public void run() {
        if ((view == null) || (view.getSDWidget()) == null) {
            return;
        }
        if (view.getSDPagingProvider() != null) {
            view.getSDPagingProvider().prevPage();
        }
        view.updateCoolBar();
        view.getSDWidget().redraw();
    }

