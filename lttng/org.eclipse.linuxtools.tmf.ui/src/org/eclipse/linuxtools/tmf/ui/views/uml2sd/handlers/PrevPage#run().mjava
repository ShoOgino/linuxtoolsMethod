    @Override
    public void run() {
        if ((fView == null) || (fView.getSDWidget()) == null) {
            return;
        }
        if (fView.getSDPagingProvider() != null) {
            fView.getSDPagingProvider().prevPage();
        }
        fView.updateCoolBar();
        fView.getSDWidget().redraw();
    }

