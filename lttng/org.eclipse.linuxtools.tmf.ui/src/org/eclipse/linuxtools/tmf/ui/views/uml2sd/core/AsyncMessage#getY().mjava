    @Override
    public int getY() {
        if ((getStartLifeline() != null) && (getEndLifeline() != null)) {
            return getEndLifeline().getY() + getEndLifeline().getHeight() + (Metrics.getMessageFontHeigth() + Metrics.getMessagesSpacing()) * getStartOccurrence();
        }
        return super.getY();
    }

