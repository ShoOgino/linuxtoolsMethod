    /*
     * (non-Javadoc)
     * @see org.eclipse.jface.dialogs.Dialog#cancelPressed()
     */
    @Override
    public void cancelPressed() {
        if (find) {
            copyToCriteria();
            if (provider instanceof ISDFindProvider) {
                ((ISDFindProvider) provider).cancel();
            }
            saveCriteria();
        }
        super.cancelPressed();
    }

