     @Override
    public void run() {
        if ((getView() == null) || (getView().getSDWidget()) == null) {
            return;
        }
        if (getView().getSDPagingProvider() != null) {
            getView().getSDPagingProvider().nextPage();
        }
        getView().updateCoolBar();
        getView().getSDWidget().redraw();
    }

