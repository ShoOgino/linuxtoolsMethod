    /*
     * (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.ui.views.uml2sd.ScrollView#contentsToViewY(int)
     */
    @Override
    public int contentsToViewY(int _y) {
        if (printing) {
            int v = Math.round(printerY * printerZoom);
            return _y - v;
        }
        return _y - getContentsY();
    }

