    public void refreshData(Object traces[]) {
        if (traces == null || traces.length == 0) {
            traceFilter = null;
        } else if (traceFilter == null || traces.length != traceFilter.length) {
            traceFilter = new boolean[traces.length];
            java.util.Arrays.fill(traceFilter, true);
        }

        _traces = traces;
        refreshData();
    }

