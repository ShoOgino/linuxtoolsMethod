    /**
     * Handler for the Trace Opened signal
     *
     * @param signal
     *            The incoming signal
     */
    @TmfSignalHandler
    public void traceOpened(TmfTraceOpenedSignal signal) {
        if (!signal.getTrace().getResource().equals(getResource())) {
            return;
        }

        refreshAnalysis();
        getParent().refresh();
    }

