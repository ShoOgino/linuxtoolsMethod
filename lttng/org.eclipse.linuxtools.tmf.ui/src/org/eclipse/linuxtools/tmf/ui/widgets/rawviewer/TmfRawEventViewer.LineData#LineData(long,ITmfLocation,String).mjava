        public LineData(long rank, ITmfLocation location, String string) {
            this.rank = rank;
            this.location = location;
            if (string.length() == 0) {
                this.string = " "; // workaround for setLineBackground has no effect on empty line //$NON-NLS-1$
            } else {
                this.string = string;
            }
        }

