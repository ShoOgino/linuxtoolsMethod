	private boolean isTmfProject(IProject project) {
	    if (project.isOpen()) {
	        IProjectNature nature;
            try {
                nature = project.getNature(TmfProjectNature.ID);
                if (nature instanceof TmfProjectNature) {
                    return true;
                }
            } catch (CoreException e) {
                e.printStackTrace();
            }
	    }
	    return false;
	}

