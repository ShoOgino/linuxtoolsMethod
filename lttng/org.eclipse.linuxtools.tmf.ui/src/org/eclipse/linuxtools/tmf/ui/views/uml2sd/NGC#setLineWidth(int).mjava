    /*
     * (non-Javadoc)
     * @see org.eclipse.linuxtools.tmf.ui.views.uml2sd.drawings.IGC#setLineWidth(int)
     */
    @Override
    public void setLineWidth(int width) {
        if (view.isPrinting()) {
            context.setLineWidth(width * 2);
        }
        else {
            context.setLineWidth(width);
        }
    }

