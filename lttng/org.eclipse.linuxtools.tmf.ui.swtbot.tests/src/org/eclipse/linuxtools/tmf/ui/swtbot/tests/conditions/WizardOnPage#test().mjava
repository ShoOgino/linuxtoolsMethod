    @Override
    public boolean test() throws Exception {
        if (fWizard == null || fPage == null) {
            return false;
        }
        final IWizardContainer container = fWizard.getContainer();
        if (container == null) {
            return false;
        }
        IWizardPage currentPage = container.getCurrentPage();
        return fPage.equals(currentPage);
    }

