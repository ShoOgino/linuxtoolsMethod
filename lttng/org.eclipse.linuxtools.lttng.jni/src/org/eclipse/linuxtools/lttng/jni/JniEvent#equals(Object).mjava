    @Override
	public boolean equals(Object other) {
        if (this == other)
             return true;
        if (other == null)
            return false;
		if (!(other instanceof JniEvent))
		    return false;
		JniEvent event = (JniEvent) other;
		return (getEventTime().equals(event.getEventTime())
				&&	parentTracefile.equals(event.parentTracefile));
	}

