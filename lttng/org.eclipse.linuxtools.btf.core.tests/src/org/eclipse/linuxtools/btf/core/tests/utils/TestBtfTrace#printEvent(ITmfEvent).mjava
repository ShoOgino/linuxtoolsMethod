    private static void printEvent(ITmfEvent event) {
        if (event == null) {
            System.out.println("null");
        } else {
            System.out.println(event.getRank() + " " + event.getTimestamp().getValue() + " " + event.getSource() + " " + event.getType().getName() + " " + event.getContent().toString());
        }
    }

