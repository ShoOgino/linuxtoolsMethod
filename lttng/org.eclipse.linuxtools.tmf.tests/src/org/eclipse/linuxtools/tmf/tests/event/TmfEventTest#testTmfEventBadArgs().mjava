	public void testTmfEventBadArgs() {
		try { // Bad timestamp
			new TmfEvent(null, fSource, fType, fReference);
			fail("null copy");
		}
		catch (IllegalArgumentException e) {
			// Success
		}

		try { // Bad source
			new TmfEvent(fTimestamp1, null, fType, fReference);
			fail("null copy");
		}
		catch (IllegalArgumentException e) {
			// Success
		}

		try { // Bad type
			new TmfEvent(fTimestamp1, fSource, null, fReference);
			fail("null copy");
		}
		catch (IllegalArgumentException e) {
			// Success
		}
	}

