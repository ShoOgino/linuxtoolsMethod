    /**
     * @param filename
     * @param parser
     * @param cacheSize
     * @throws FileNotFoundException
     */
    public TmfEventStream(String filename, ITmfEventParser parser, int cacheSize) throws FileNotFoundException {
        super(filename, "r");
        assert(parser != null);
        fParser = parser;
        fCacheSize = cacheSize;
        bookmark();
    }

