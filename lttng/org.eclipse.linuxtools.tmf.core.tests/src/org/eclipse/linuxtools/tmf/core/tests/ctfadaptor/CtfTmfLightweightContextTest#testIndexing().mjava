    /**
     * Index all the events in the test trace.
     */
    @Test
    public void testIndexing() {
        CtfTmfLightweightContext context = new CtfTmfLightweightContext(fixture);
        context.seek(0);

        int count = 0;
        while (fixture.getNext(context) != null) {
            count++;
        }
        assertTrue(count > 0);
    }

