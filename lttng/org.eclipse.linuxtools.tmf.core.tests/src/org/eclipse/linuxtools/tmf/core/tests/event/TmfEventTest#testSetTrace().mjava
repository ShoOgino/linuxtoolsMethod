    public void testSetTrace() {
        ITmfTrace<TmfEvent> trace = setupTrace();
        assertNotNull(trace);

        TestEvent event = new TestEvent(fEvent1);
        assertNull("setTrace", event.getTrace());

        event.setTrace(trace);
        assertEquals("setTrace", trace, event.getTrace());

        event.setTrace(null);
        assertNull("setTrace", event.getTrace());
        
        trace.dispose();
    }

