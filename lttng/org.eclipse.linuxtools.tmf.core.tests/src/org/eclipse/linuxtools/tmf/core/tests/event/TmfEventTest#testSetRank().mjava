    public void testSetRank() {
        final TestEvent event = new TestEvent(fEvent1);
        assertEquals("setRank", 0, event.getRank());

        event.setRank(1);
        assertEquals("setRank", 1, event.getRank());

        event.setRank(-1);
        assertEquals("setRank", -1, event.getRank());

        event.setRank(0);
        assertEquals("setRank", 0, event.getRank());
    }

