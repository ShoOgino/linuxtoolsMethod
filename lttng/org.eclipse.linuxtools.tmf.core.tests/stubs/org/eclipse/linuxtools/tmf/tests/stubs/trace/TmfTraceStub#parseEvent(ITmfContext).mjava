    @Override
    public TmfEvent parseEvent(final ITmfContext context) {
        fLock.lock();
        try {
            // parseNextEvent will update the context
            if (fTrace != null) {
                final TmfEvent event = fParser.parseEvent(context.clone());
                return event;
            }
//        }
//        catch (final IOException e) {
//            e.printStackTrace();
        } finally {
            fLock.unlock();
        }
        return null;
    }

