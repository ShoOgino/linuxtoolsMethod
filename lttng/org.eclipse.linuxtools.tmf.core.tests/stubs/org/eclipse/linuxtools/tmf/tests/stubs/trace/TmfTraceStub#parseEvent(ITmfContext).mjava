    @Override
    public TmfEvent parseEvent(final ITmfContext context) {
        fLock.lock();
        try {
            // parseNextEvent will update the context
            if (fTrace != null && getParser() != null && context != null) {
                final TmfEvent event = getParser().parseEvent(context.clone());
                return event;
            }
        } finally {
            fLock.unlock();
        }
        return null;
    }

