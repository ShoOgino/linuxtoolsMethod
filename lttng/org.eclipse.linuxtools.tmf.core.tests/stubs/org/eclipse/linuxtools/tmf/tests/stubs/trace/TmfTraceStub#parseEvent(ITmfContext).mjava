    @Override
    public ITmfEvent parseEvent(final ITmfContext context) {
        fLock.lock();
        try {
            // parseNextEvent will update the context
            if (fTrace != null && getParser() != null && context != null) {
                final ITmfEvent event = getParser().parseEvent(context);
                return event;
            }
        } finally {
            fLock.unlock();
        }
        return null;
    }

