    @SuppressWarnings("unchecked")
    public TextLttngEvent(final TextLttngEvent oldEvent) {
        this(
                (TmfTrace<LttngEvent>) oldEvent.getTrace(),
                (LttngTimestamp)oldEvent.getTimestamp(),
                oldEvent.getSource(),
                oldEvent.getType(),
                oldEvent.getContent(),
                oldEvent.getReference()
                );
    }

