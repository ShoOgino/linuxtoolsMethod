	// ========================================================================
	// Constructor
	// =======================================================================
	public LttngProcessState(Long startTime, String traceId) {
		this.cpu = 0L;
		this.pid = 0L;
		this.tgid = 0L;
		this.name = StateStrings.ProcessStatus.LTTV_STATE_UNNAMED.getInName();
		this.insertion_time = startTime;
		this.trace_id = traceId;
		init();
	}

