    @Override
    public LTTngTrace copy() {
        LTTngTrace returnedTrace = null;

        try {
            returnedTrace = new LTTngTrace(this);
        } catch (Exception e) {
            System.out.println("ERROR : Could not create LTTngTrace copy (createTraceCopy)."); //$NON-NLS-1$
            e.printStackTrace();
        }

        return returnedTrace;
    }

