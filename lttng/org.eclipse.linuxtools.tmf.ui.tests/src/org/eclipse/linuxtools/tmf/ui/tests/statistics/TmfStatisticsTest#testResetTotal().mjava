    /**
     * Test of the reset for the total counter
     */
    public void testResetTotal() {
        stats.setValue(true, 123);
        assertEquals(123, stats.getTotal());

        stats.resetTotalCount();
        assertEquals(0, stats.getTotal());

        // test when already at 0
        stats.resetTotalCount();
        assertEquals(0, stats.getTotal());
    }

