    /*
     * Run the runnables of the wait list.
     */
    private void run_wait_list() {
        if (fWaitList.isEmpty()) {
            return;
        }
        Runnable[] runnables = fWaitList.toArray(new Runnable[fWaitList.size()]);
        fWaitList.clear();
        for (int i = 0; i < runnables.length; i++) {
            runnables[i].run();
        }
    }

