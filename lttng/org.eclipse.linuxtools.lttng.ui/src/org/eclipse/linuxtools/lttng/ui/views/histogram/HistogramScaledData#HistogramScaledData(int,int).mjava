    public HistogramScaledData(int width, int height) {
        fWidth = width;
        fHeight = height;
        fData = new int[width];
        Arrays.fill(fData, 0);
        fBucketDuration = 1;
        fMaxValue = 0;
        fCurrentBucket = 0;
        fLastBucket = 0;
        fScalingFactor = 1;
    }

