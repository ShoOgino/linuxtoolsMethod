    /**
     * @param event
     * @return
     */
    @Override
    protected String[] extractItemFields(TmfEvent event) {
        String[] fields = new String[0];

        if (event != null) {
            fields = new String[] {
                    event.getTimestamp().toString(),
                    event.getReference().toString(),
                    event.getType().toString(),
                    event.getContent().toString()
            };
        }
        return fields;
    }

