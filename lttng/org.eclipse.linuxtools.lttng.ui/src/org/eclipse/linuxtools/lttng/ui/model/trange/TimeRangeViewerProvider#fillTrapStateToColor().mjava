	private void fillTrapStateToColor() {
		trapStateToColor.put(TrapMode.LTTV_TRAP_UNKNOWN.getInName(),
				StateColor.BLACK);
		trapStateToColor.put(TrapMode.LTTV_TRAP_IDLE.getInName(),
				StateColor.BLACK);
		trapStateToColor.put(TrapMode.LTTV_TRAP_BUSY.getInName(),
				StateColor.GOLD);
	}

