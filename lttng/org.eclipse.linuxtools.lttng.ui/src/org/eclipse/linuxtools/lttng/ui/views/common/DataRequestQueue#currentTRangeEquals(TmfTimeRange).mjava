	/**
	 * Compare new request time range with the one related to the current
	 * request
	 * 
	 * @param trange
	 * @return
	 */
	private boolean currentTRangeEquals(TmfTimeRange trange) {
		if (currentRequest != null) {
			if (currentRequest.getRange().equals(trange)) {
				return true;
			}
		}
		return false;
	}

