    /**
     * @param title
     * @param desc
     */
    public NewProjectWizard(String title, String desc) {
        super();
        setDialogSettings(LTTngUiPlugin.getDefault().getDialogSettings());
        setNeedsProgressMonitor(true);
        setForcePreviousAndNextButtons(true);
        setWindowTitle(title);
        fTtitle = title;
        fDescription = desc;
    }

