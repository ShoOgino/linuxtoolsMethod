		eventNametoAfterProcessor.put(
				StateStrings.Events.LTT_EVENT_PROCESS_EXIT.getInName(),
				instantiateAfterHandler.getProcessExitHandler());

		eventNametoAfterProcessor.put(StateStrings.Events.LTT_EVENT_EXEC
