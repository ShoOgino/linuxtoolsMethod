		eventNametoAfterProcessor.put(
				StateStrings.Events.LTT_EVENT_SCHED_SCHEDULE.getInName(),
				instantiateAfterHandler.getSchedChangeHandler());

		eventNametoAfterProcessor.put(
				StateStrings.Events.LTT_EVENT_PROCESS_FORK.getInName(),
