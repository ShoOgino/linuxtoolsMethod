		eventNametoAfterProcessor.put(
				StateStrings.Events.LTT_EVENT_THREAD_BRAND.getInName(),
				instantiateAfterHandler.GetThreadBrandHandler());

		eventNametoAfterProcessor.put(
				StateStrings.Events.LTT_EVENT_PROCESS_STATE.getInName(),
