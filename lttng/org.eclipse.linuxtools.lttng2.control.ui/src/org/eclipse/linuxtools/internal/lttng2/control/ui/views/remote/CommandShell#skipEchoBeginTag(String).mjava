    private static int skipEchoBeginTag(String output) {
        final String searchFor = SHELL_ECHO_CMD + BEGIN_TAG;
        int begin = 0;
        for(;;) {
            int i= output.indexOf(searchFor, begin);
            if (i >= begin) {
                begin = i + searchFor.length();
            } else {
                return begin;
            }
        }
    }

