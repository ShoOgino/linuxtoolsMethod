	/**
	 * Return the require paths
	 *
	 * @return The require paths
	 */
	public List<String> getRequirePaths() {
		List<String> rc = new ArrayList<String>();
		List<String> temp = getValueList(REQUIRE_PATHS);

		if (!temp.isEmpty()) {
			for (int i = 0; i < temp.size(); i++) {
				rc.add(temp
						.get(i)
						.replaceFirst("(?:%q|%Q)(?:([\\W])([^\\W]+)[\\W])",
								"$2").replaceAll("(\"|'|\\[|\\])", "")
						.replaceAll(",", ""));
			}
		}

		return rc;
	}

