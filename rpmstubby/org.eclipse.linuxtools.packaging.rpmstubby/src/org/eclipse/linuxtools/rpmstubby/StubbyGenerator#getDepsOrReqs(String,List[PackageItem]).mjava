	private String getDepsOrReqs(String preString, List<PackageItem> packageItems) {
		String toRet = "";
		for (PackageItem packageItem: packageItems) {
			toRet += preString + packageItem.getName() + " "
			+ packageItem.getOperator() + " "
			+ packageItem.getVersion() + "\n";
		}
		return toRet;
	}

