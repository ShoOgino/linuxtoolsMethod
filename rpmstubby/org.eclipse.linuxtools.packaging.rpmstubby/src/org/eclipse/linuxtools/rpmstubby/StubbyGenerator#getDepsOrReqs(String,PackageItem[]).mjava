	private String getDepsOrReqs(String preString, PackageItem[] packageItems) {
		String toRet = "";
		for (int i = 0; i < packageItems.length; i++) {
			toRet += preString + packageItems[i].getName() + " "
			+ packageItems[i].getOperator() + " "
			+ packageItems[i].getVersion() + "\n";
		}
		return toRet;
	}

